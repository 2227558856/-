                              <script>
                                  $(document).ready(function (){
                                      $("#sub").click(function (){
                                          if($("#username").val()==null||$("#username").val()===""){
                                              alert("用户名不能为空!");
                                              return false;
                                          }
                                          else if(!/^[\u4e00-\u9fa5]{2,4}$/.test($("#username").val())){
                                              /*用户名由2~4个中文字符构成*/
                                              alert("用户名格式不正确!");
                                              return false;
                                          }
                                          else if($("#password").val()==null||$("#password").val()===""){
                                              alert("密码不能为空!");
                                              return false;
                                          }
                                          else if(!/^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{10,18}$/.test($("#password").val())){
                                              /*由数字和字母组成，并且要同时含有数字和字母，且长度要在10-18位之间。*/
                                              alert("密码格式不正确!");
                                              return false;
                                          }
                                          else if($("#email").val()==null||$("#email").val()===""){
                                              alert("E-mail不能为空!");
                                              return false;
                                          }
                                          else if(!/^\w+@[a-z0-9]+(\.[a-z]+){1,3}$/.test($("#email").val())){
                                              alert("E-mail格式不正确!");
                                              return false;
                                          }
                                          else if($("#phone").val()==null||$("#phone").val()===""){
                                              alert("手机号不能为空!");
                                              return false;
                                          }
                                          else if(!/^[1]([3-9])[0-9]{9}$/.test($("#phone").val())){
                                              alert("手机号格式不正确!");
                                              return false;
                                          }
                                          else if($('input[name="gender"]').filter(':checked').val() == undefined){
                                              alert("请选择性别!");
                                              return false;
                                          }
                                          else if($("#birthday").val()==""){
                                              alert("请选择出生日期!");
                                              return false;
                                          }
                                      })
                                  })
                              </script>
                              </body>
                              </html>



18.                           
